def get_word():
    return random.choice(word_list).upper()

def display_hangman(trying):
    stages = [  # финальное состояние: голова, торс, обе руки, обе ноги
        '''
           --------
           |      |
           |      O
           |     \\|/
           |      |
           |     / \\
           -
        ''',
        # голова, торс, обе руки, одна нога
        '''
           --------
           |      |
           |      O
           |     \\|/
           |      |
           |     / 
           -
        ''',
        # голова, торс, обе руки
        '''
           --------
           |      |
           |      O
           |     \\|/
           |      |
           |      
           -
        ''',
        # голова, торс и одна рука
        '''
           --------
           |      |
           |      O
           |     \\|
           |      |
           |     
           -
        ''',
        # голова и торс
        '''
           --------
           |      |
           |      O
           |      |
           |      |
           |     
           -
        ''',
        # голова
        '''
           --------
           |      |
           |      O
           |    
           |      
           |     
           -
        ''',
        # начальное состояние
        '''
           --------
           |      |
           |      
           |    
           |      
           |     
           -
        '''
    ]
    return stages[trying]


def play():
    print("Давай играть в угадайку слов!")
    word = get_word()
    word_completion = "_" * len(word)
    print(word_completion)
    guessed = False
    guessed_letters = []
    tries = 6
    print(display_hangman(tries))
    while guessed == False:
        a_try = input().upper()
        if a_try.isalpha() and len(a_try) == 1:
            if a_try in word and  a_try not in guessed_letters:
                indexes = [i for i,c in enumerate(word) if c == a_try]
                for i in indexes:
                    word_completion = word_completion[:i]+a_try+word_completion[i+1:]
                guessed_letters.append(a_try)
                print(word_completion)
                print(guessed_letters)
                print(f"Осталось {tries} попыток")
            elif a_try not in word:
                tries -=1
                print("Такой буквы нет(")
                guessed_letters.append(a_try)
                print(word_completion)
                print(guessed_letters)
                print(display_hangman(tries))
                print(f"Осталось {tries} попыток")
            else:
                print("Такая буква уже была использована, попробуй другую")
        else:
            print("invalid input, try again!")
        if word == word_completion and tries != 0:
            print('You won, congratulations!')
            guessed = True
        if word != word_completion and tries == 0:
            print("you lost(")
            break



play()
